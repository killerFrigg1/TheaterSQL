<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="320.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.ModifyFilmController">
	<children>
		<VBox alignment="CENTER" layoutX="79.0" layoutY="84.0"
			prefHeight="480.0" prefWidth="320.0" spacing="15.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label layoutX="155.0" layoutY="105.0" text="ID Filmu">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<TextField fx:id="id" layoutX="20.0" layoutY="120.0" />
				<Label text="Tytuł">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<TextField fx:id="nazwa" />
				<Label text="Opis">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<TextField fx:id="opis" />
				<Label text="Czas trwania">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<TextField fx:id="czasTrwania" />
				<Label text="Limit wiekowy">
					<font>
						<Font size="14.0" />
					</font>
				</Label>
				<TextField fx:id="limitWiekowy" />
				<Button fx:id="removeButton" mnemonicParsing="false"
					onAction="#remove" prefWidth="200.0" text="Usuń (wystarczy wpisać ID)">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
				<Button fx:id="modifyButton" layoutX="70.0" layoutY="423.0"
					mnemonicParsing="false" onAction="#modify" prefWidth="200.0" text="Zmień">
					<font>
						<Font size="14.0" />
					</font>
				</Button>
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</VBox>
	</children>
</AnchorPane>
